		"fractils": "^2.0.3",
		"svelte-ethers-store": "^2.5.10",
		"svelte-fa": "^3.0.3",
		"svelte-reveal": "^0.6.0",
		"svelte-typewriter": "^3.0.0-alpha.11",
		"tailwindcss-border-gradient-radius": "^3.0.1",
		"theme-change": "^2.1.0",
		"ts-node": "^10.8.2"
	}
